pipeline {
	agent none
	environment {
	test='temp empty file'
	test1='check file' }
		stages {
			stage ('step1') {
			agent label 'slave2'
				steps {
				sh "echo $test"
				}
			}
			stage ('test all') {
			parallel {
			stage ('step2') {
			agent label 'master'
				step {
				sh pwd
				ls
				sh "echo $test1"
				}
			}
			stage ('step3') {
			agent label 'slave2'
				steps {
				sh 'sleep 2'
				"echo $test"
				}
			}
			}
		}
		}
		
		
}
