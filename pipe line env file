pipeline {
	agent none
	environment {
	test= 'test madi file'
	test1= 'test3'
	}
		stages {
			stage ('build') {
				agent {label 'slave2'}
				steps {
				sh '''
				ls
				pwd
				date 
				'''
				}
			}
			stage('test all') {
			parallel {
			stage ('test') {
			agent {label 'master'}
				steps {
				sh ''' 
				ls 
				pwd
				'''
				}
			}
			stage ('deploy') {
			agent {label 'slave2'}
				steps {
				sh '''
				echo $test
				'''
				}
			}
			}
		}
		}
		
	}
